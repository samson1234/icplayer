<?xml version='1.0' encoding='UTF-8' ?>

<addon id='IWB_Toolbar' xmlns="http://icplayer.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://icplayer.com ../../tools/addon.xsd">
	<model>
        <property name="floatingImages" displayName="Floating Images" type="list">
            <property name="Image" type="file" />
        </property>
        <property name="widthWhenOpened" displayName="Width When Opened" type="string" />
        <property name="Fixed Position" type="boolean"/>

        <property name="forPen" displayName="Show for pen" type="{---, Color panel, Thickness panel}" />
        <property name="forMarker" displayName="Show for marker" type="{---, Color panel, Thickness panel}" />
	</model>
</addon>